/* ---- FCN_VO(tgGM_ST_Calc_Space) ----------------------------------------------------------------------------------------------------------------------------------------------- */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
TgINLINE TgVOID FCN_VO(tgGM_ST_Calc_Space)( VEC_OBJ_T(TgSTRI,PCU) psST0 )
{
    VEC_T(1,C) vN0 = FCN_V(tgMH_UCX)( &psST0->m_sCT.m_sET.m_sPT.m_vNormal, psST0->m_sCT.m_sET.m_avEdge + 0 );
    VEC_T(1,C) vN1 = FCN_V(tgMH_UCX)( &psST0->m_sCT.m_sET.m_sPT.m_vNormal, psST0->m_sCT.m_sET.m_avEdge + 1 );
    VEC_T(1,C) vN2 = FCN_V(tgMH_UCX)( &psST0->m_sCT.m_sET.m_sPT.m_vNormal, psST0->m_sCT.m_sET.m_avEdge + 2 );

    FCN_VO(tgGM_PN_Init_NP)( psST0->m_avPlane + 0, &vN0, psST0->m_sCT.m_sET.m_sPT.m_avPoint + 0 );
    FCN_VO(tgGM_PN_Init_NP)( psST0->m_avPlane + 1, &vN1, psST0->m_sCT.m_sET.m_sPT.m_avPoint + 1 );
    FCN_VO(tgGM_PN_Init_NP)( psST0->m_avPlane + 2, &vN2, psST0->m_sCT.m_sET.m_sPT.m_avPoint + 2 );
}


