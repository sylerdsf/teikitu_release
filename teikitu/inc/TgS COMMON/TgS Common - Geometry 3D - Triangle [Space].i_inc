/* ---- FCN_VO(tgGM_ST_Is_Contained) --------------------------------------------------------------------------------------------------------------------------------------------- */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
TgINLINE TgBOOL FCN_VO(tgGM_ST_Is_Contained)( VEC_OBJ_T(TgSTRI,CPCU) psST1, VEC_T(1,CPCU) pvS0 )
{
    return (
        FCN_VO(tgGM_F_Dist_PN_VT)(psST1->m_avPlane + 0, pvS0) >= TYPE_K(0) &&
        FCN_VO(tgGM_F_Dist_PN_VT)(psST1->m_avPlane + 1, pvS0) >= TYPE_K(0) &&
        FCN_VO(tgGM_F_Dist_PN_VT)( psST1->m_avPlane + 2, pvS0) >= TYPE_K(0,0)
    );
}


