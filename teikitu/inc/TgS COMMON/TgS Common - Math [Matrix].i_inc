/* ---- FCN_M(tgMH_CLR) ---------------------------------------------------------------------------------------------------------------------------------------------------------- */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
TgINLINE TgVOID FCN_M(tgMH_CLR)( MAT_T(PCU) pmM0 )
{
    tgMM_Set_U08_0x00( pmM0, sizeof( MAT_T() ) );
}


/* ---- FCN_M(tgMH_MAX) ---------------------------------------------------------------------------------------------------------------------------------------------------------- */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
TgINLINE TgVOID FCN_M(tgMH_MAX)( MAT_T(PCU) pmOUT_0, MAT_T(CPCU) pmM0, MAT_T(CPCU) pmM1 )
{
    #define EQN(A) FCN_V(tgMH_MAX)( pmM0->m_avCol[A], pmM1->m_avCol[A] )
    GENERATE__MAT_ASSIGN
    #undef EQN
}


