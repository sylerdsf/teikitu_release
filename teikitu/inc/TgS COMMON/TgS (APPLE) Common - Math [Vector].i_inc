{
    TgUN_V128                           uResult;

    VEC_S_UN_1_MEMBER(uResult) = FCN_V(tgMH_ADD_CLP_S)( *(VEC_S_T(1,CP))&vLeft, *(VEC_S_T(1,CP))&vRight );
    return (VEC_UN_1_MEMBER(uResult));
}
#endif


