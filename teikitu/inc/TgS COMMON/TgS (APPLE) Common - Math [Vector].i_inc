/* ---- FCN_V(tgMH_PERM) --------------------------------------------------------------------------------------------------------------------------------------------------------- */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
TgINLINE VEC_T(1) FCN_V(tgMH_PERM)( VEC_T(1,C) v0, VEC_T(1,C) v1, VEC_T(1,C) vMask )
{
    TgUN_V128                           uResult;

    VEC_S_UN_1_MEMBER(uResult) = FCN_V(tgMH_PERM_S)( *(VEC_S_T(1,CP))&v0, *(VEC_S_T(1,CP))&v1, (UVAR_T(CP))&vMask );
