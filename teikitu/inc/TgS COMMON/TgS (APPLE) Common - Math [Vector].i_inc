/* ---- FCN_V(tgMH_RAND) --------------------------------------------------------------------------------------------------------------------------------------------------------- */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
TgINLINE VEC_T(1) FCN_V(tgMH_RAND)( TgVOID )
{
    TgUN_V128                           uResult;

    VEC_S_UN_1_MEMBER(uResult) = FCN_V(tgMH_RAND_S)();
