/* ---- FCN_V(tgMH_CMP_TO_BOOL_N) ------------------------------------------------------------------------------------------------------------------------------------------------ */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
TgINLINE TgBOOL FCN_V(tgMH_CMP_TO_BOOL_N)( VEC_N_T(1,C) v0 )
{
    TgUN_V128                           vResult;

    VAR_UN_1_MEMBER(vResult.m_N_) = v0;
    if (   (vResult.m_U32_04_1.x == KTgFFFF_V128.m_N_sV.m_U32_04_1.x)
        && (vResult.m_U32_04_1.y == KTgFFFF_V128.m_N_sV.m_U32_04_1.y)
        && (vResult.m_U32_04_1.z == KTgFFFF_V128.m_N_sV.m_U32_04_1.z)
        && (vResult.m_U32_04_1.w == KTgFFFF_V128.m_N_sV.m_U32_04_1.w) )
    {
        return true;
    }
    else
    {
        return false;
    };
}
