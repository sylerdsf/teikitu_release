/* ---- FCN_V(tgMH_NAN) ---------------------------------------------------------------------------------------------------------------------------------------------------------- */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
CLANG_WARN_DISABLE_PUSH(float-equal)
TgINLINE TgBOOL FCN_V(tgMH_NAN)( VEC_T(1,C) vARG_0 )
{
    VEC_UN_T(1,C)   uResult = { VEC_UN_1_MEMBER() = vARG_0 };

    for (TgRSIZE uiIndex = 0; uiIndex < TEMPLATE__VECTOR_DIM; ++uiIndex)
    {
        if (!(VEC_S_UN_1_MEMBER(uResult).m_aData[uiIndex] == VEC_S_UN_1_MEMBER(uResult).m_aData[uiIndex]))
        {
            return (true);
        };
    };

    return (false);
}
CLANG_WARN_DISABLE_POP(float-equal)


/* ---- FCN_V(tgMH_BND) ---------------------------------------------------------------------------------------------------------------------------------------------------------- */
/* ------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- */
TgINLINE VEC_T(1) FCN_V(tgMH_BND)( VEC_T(1,C) vARG_0, VEC_T(1,C) vBound )
{
    VEC_UN_T(1)                         uResult;

    VEC_UN_T(1,C)   uV0 = { VEC_UN_1_MEMBER() = vARG_0 };
    VEC_UN_T(1,C)   uBound = { VEC_UN_1_MEMBER() = vBound };

    for (TgRSIZE uiIndex = 0; uiIndex < TEMPLATE__VECTOR_DIM; ++uiIndex)
    {
        if (
               ( -VEC_S_UN_1_MEMBER(uBound).m_aData[uiIndex] > VEC_S_UN_1_MEMBER(uV0).m_aData[uiIndex] )
            || (  VEC_S_UN_1_MEMBER(uV0).m_aData[uiIndex] > VEC_S_UN_1_MEMBER(uBound).m_aData[uiIndex] ) )
        {
            tgMM_Set_U08_0x00( VEC_S_UN_1_MEMBER(uResult).m_aData + uiIndex, sizeof( VAR_T() ) );
        }
        else
        {
            tgMM_Set_U08_0xFF( VEC_S_UN_1_MEMBER(uResult).m_aData + uiIndex, sizeof( VAR_T() ) );
        };
    };

    return (VEC_UN_1_MEMBER(uResult));
}


